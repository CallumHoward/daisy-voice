{
  "name": "sanity-template-sveltekit-clean",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "pnpm -r dev",
    "format": "prettier --cache --write .",
    "pretypegen": "pnpm -F studio exec sanity schema extract --enforce-required-fields",
    "typegen": "pnpm -F studio exec sanity typegen generate"
  },
  "prettier": {
    "plugins": [
      "prettier-plugin-packagejson"
    ]
  },
  "devDependencies": {
    "prettier": "3.2.5",
    "prettier-plugin-packagejson": "2.5.0"
  },
  "packageManager": "pnpm@9.0.6"
}
